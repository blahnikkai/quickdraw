{
    "compilerOptions": {
        "target": "es6",
        "jsx": "react-jsx",
        "module": "system",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "noImplicitReturns": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "outFile": "../../built/local/tsc.js",
        "esModuleInterop": true,
        "sourceMap": true,
    },
}